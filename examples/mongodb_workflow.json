{
  "id": "workflow-2",
  "name": "Adult User Registration",
  "nodes": [
    {
      "id": "start-1",
      "type": "start",
      "config": {
        "initialData": {
          "name": "Alice Smith",
          "age": 30,
          "email": "alice@example.com",
          "country": "USA",
          "minAge": 18
        }
      }
    },
    {
      "id": "age-check",
      "type": "condition",
      "config": {
        "lhs": "{{age}}",
        "operator": ">=",
        "rhs": "{{minAge}}"
      }
    },
    {
      "id": "insert-adult",
      "type": "mongodb_insert",
      "config": {
        "database": "workflow_db",
        "collection": "adult_users",
        "document": {
          "name": "{{name}}",
          "age": "{{age}}",
          "email": "{{email}}",
          "country": "{{country}}",
          "accountType": "full",
          "createdAt": "2025-10-07T10:30:00Z"
        }
      }
    }
  ],
  "edges": [
    {
      "from": "start-1",
      "to": "age-check",
      "output": "default"
    },
    {
      "from": "age-check",
      "to": "insert-adult",
      "output": "true"
    }
  ]
}

